{
  "name": "t-test",
  "version": "1.1.0",
  "description": "Angular t-Test-Rechner",
  "main": "index.js",
  "scripts": {
    "test": "karma start --single-run",
    "prebuild": "rm -f dist/*",
    "build:jade": "jade src/index.jade -o dist",
    "build:js": "NODE_ENV=production webpack -p",
    "build:compress": "zopfli dist/*",
    "build": "npm run build:js && npm run build:jade && npm run build:compress",
    "preversion": "npm test && npm run build && git add ./dist",
    "postversion": "git push && git push --tags"
  },
  "author": "Tamara Cook <tamara.cook@icloud.com>",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/tamaracha/t-test.git"
  },
  "devDependencies": {
    "angular": "^1.5.0-rc.1",
    "angular-animate": "^1.5.0-rc.1",
    "angular-aria": "^1.5.0-rc.1",
    "angular-mocks": "^1.5.0-rc.1",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "big.js": "^3.1.3",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.0",
    "isparta-instrumenter-loader": "^1.0.0",
    "jade": "^1.11.0",
    "jasmine-core": "^2.4.1",
    "jstat": "^1.5.2",
    "karma": "^0.13.19",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-safari-launcher": "^0.1.1",
    "karma-webpack": "^1.7.0",
    "lodash.pick": "^4.0.0",
    "ng-annotate-loader": "^0.1.0",
    "ngtemplate-loader": "^1.3.0",
    "node-zopfli": "^1.4.0",
    "phantomjs": "^1.9.19",
    "phantomjs-polyfill": "0.0.1",
    "style-loader": "^0.13.0",
    "template-html-loader": "0.0.3",
    "webpack": "^1.12.12"
  }
}
